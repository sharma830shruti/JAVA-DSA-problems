
import java.io.*;
import java.util.*;
import static java.lang.System.out;

class Main {

	
	static int countDigits(int num) {
		int count = 0;
     	while(num > 0) {
		num = num / 10;
          count++;	
     	}	
         return count;
	}

	public static void main (String[] args) {
		int number = 789;
    	System.out.println(countDigits(number));
	}
}
